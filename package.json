{
  "name": "root",
  "version": "0.0.5",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "cd packages/admin && npm start",
    "dev:www": "cd packages/www && npm start",
    "dev:admin": "cd packages/admin && npm start",
    "build": "cd packages/admin && npm run build",
    "build:www": "cd packages/www && npm run build",
    "build:admin": "cd packages/admin && npm run build",
    "start:api": "cd .. && dotnet run --launch-profile ApiOnly",
    "init": "yarn install && lerna bootstrap",
    "docs": "docsify serve docs",
    "clean": "lerna clean",
    "clean-all": "npm run clean && rimraf node_modules",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "lerna": "^3.22.1",
    "webpack": "4.42.0"
  }
}
